{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": ["../../.eslintrc.json"],
	"rules": {
		"import/order": [
			"error",
			{
				"groups": [
					"builtin",
					"external",
					"internal",
					"parent",
					"sibling",
					"index",
					"object",
					"type"
				],
				"pathGroups": [
					{
						"pattern": "@/**",
						"group": "internal"
					}
				]
			}
		]
	}
}
